; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

_global_script_classes=[{
"base": "PanelContainer",
"class": &"InventoryContainer",
"language": &"GDScript",
"path": "res://Code/UI/InventoryContainer.gd"
}, {
"base": "Resource",
"class": &"Item",
"language": &"GDScript",
"path": "res://Code/Resources/Prefabs/item.gd"
}, {
"base": "PanelContainer",
"class": &"ItemContainer",
"language": &"GDScript",
"path": "res://Code/UI/ItemContainer.gd"
}]
_global_script_class_icons={
"InventoryContainer": "",
"Item": "",
"ItemContainer": ""
}

[application]

config/name="HellAgency"
run/main_scene="res://UI/Pages/mockup.tscn"
config/features=PackedStringArray("4.0", "Mobile")
run/max_fps=60
config/icon="res://icon.svg"

[display]

window/size/viewport_width=270
window/size/viewport_height=480
window/size/window_width_override=540
window/size/window_height_override=960
window/handheld/orientation=1
window/stretch/mode="canvas_items"
window/stretch/aspect="ignore"

[input_devices]

pointing/emulate_touch_from_mouse=true

[rendering]

renderer/rendering_method="mobile"
